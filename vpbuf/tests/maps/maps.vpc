target  language cplusplus 1-1 vp_maps "./cpp/src" cpp
        language python 1-1 vp_maps "./python/vp_maps" py
        language javascript 1-1 vp_maps "./nodejs/src" js

pod Header
    varint version 1
    string test_name 1

pod A
    varint i1 1
    string s1 1

pod OuterA
    map varint A lookup 1

pod OuterB
    map string A lookup 1

pod OuterC
    map varint string lookup 1

pod D1
    A apod 1
    varint i 1
    string s 1
    *A aref 1

pod OuterD
    map string D1 lookup 1

rpod Egg : h1 (EggScrambler)
    varint i1 1
    string s1 1
    salt SaltShaker (string)
    salt PepperShaker (string)

pod Omlette
    map varint Egg lookup 1